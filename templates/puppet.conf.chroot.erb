[main] 
    report = true
    reports = log,store
    vardir = /etc/puppet/var
    logdir = /etc/puppet/var/log
    certname = <%= @hostname %>-<%= @image %>.woods.ccrc

[agent]
    report = true
    server = aarya.woods.ccrc
    environment = <%= @environment %>
